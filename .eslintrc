{
  "parser": "babel-eslint",
  "extends": [
    "airbnb/hooks",
    "airbnb",
    "plugin:prettier/recommended",
    "prettier/standard",
    "prettier/react"
  ],
  "env": {
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 2020,
    "ecmaFeatures": { "jsx": true }
  },
  "settings": {
    "react": {
      "version": "16"
    }
  },
  "rules": {
    "react/prop-types": 0,
    "react/jsx-filename-extension": 0,
    "react/jsx-fragments": 0
  }
}
